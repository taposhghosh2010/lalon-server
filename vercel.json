{
    "version": 2,
    "buildCommand": "pnpm run build",
    "builds": [
        {
            "src" : "dist/vercel/index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes" : [
        {
            "src": "/(.*)",
            "dest": "dist/vercel/index.js"
        }
    ],
    "outputDirectory": "dist"
}
